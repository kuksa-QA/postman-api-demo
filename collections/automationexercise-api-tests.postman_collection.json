			"name": "Products",
			"item": [
				{
					"name": "Get All Products",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response has responseCode and products\", function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.have.property(\"responseCode\");\r",
									"    pm.expect(jsonData).to.have.property(\"products\");\r",
									"});\r",
									"\r",
									"pm.test(\"Business responseCode is 200\", function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.responseCode).is.to.eql(200)\r",
									"});\r",
									"\r",
									"pm.test(\"Products is a non-empty array\", function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.products).to.be.an(\"array\");\r",
									"    pm.expect(jsonData.products.length).to.be.greaterThan(0);\r",
									"});\r",
									"\r",
									"pm.test(\"Each product has required fields\", function () {\r",
									"    const products = pm.response.json().products;\r",
									"\r",
									"    products.forEach(product => {\r",
									"        pm.expect(product).to.have.property(\"id\");\r",
									"        pm.expect(product).to.have.property(\"name\");\r",
									"        pm.expect(product).to.have.property(\"price\");\r",
									"        pm.expect(product).to.have.property(\"brand\");\r",
									"        pm.expect(product).to.have.property(\"category\");\r",
									"    });\r",
									"});\r",
									"\r",
									"pm.test(\"Each product has valid category structure\", function () {\r",
									"    const products = pm.response.json().products;\r",
									"\r",
									"    products.forEach(product => {\r",
									"        pm.expect(product.category).to.have.property(\"category\");\r",
									"        pm.expect(product.category).to.have.property(\"usertype\");\r",
									"        pm.expect(product.category.usertype).to.have.property(\"usertype\");\r",
									"    });\r",
									"});\r",
									"\r",
									"pm.test(\"Product fields have correct data types\", function () {\r",
									"    const products = pm.response.json().products;\r",
									"\r",
									"    products.forEach(product => {\r",
									"        pm.expect(product.id).to.be.a(\"number\");\r",
									"        pm.expect(product.name).to.be.a(\"string\");\r",
									"        pm.expect(product.price).to.be.a(\"string\"); \r",
									"        pm.expect(product.brand).to.be.a(\"string\");\r",
									"        pm.expect(product.category.category).to.be.a(\"string\");\r",
									"        pm.expect(product.category.usertype.usertype).to.be.a(\"string\");\r",
									"    });\r",
									"});\r",
									"\r",
									"pm.test(\"Product IDs are unique\", function () {\r",
									"    const ids = pm.response.json().products.map(p => p.id);\r",
									"    const uniqueIds = new Set(ids);\r",
									"\r",
									"    pm.expect(uniqueIds.size).to.eql(ids.length);\r",
									"});\r",
									"\r",
									"pm.test(\"Response time is under 1000ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
									"});\r",
									"\r",
									""
								],
								"type": "text/javascript"
							}
